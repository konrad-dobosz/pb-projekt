@model IEnumerable<pb_projekt.Models.LandShipment>

@{
    ViewData["Title"] = "Land Shipments";
}

<h2>Land Shipments</h2>

<table>
    <tr>
        <th>Id</th>
        <th>Actions</th>
    </tr>

    @foreach (var landShipment in Model)
    {
            <tr>
                <td>@landShipment.Id</td>
                <td>
                    <a href="@Url.RouteUrl("LandShipmentCargo", new { id = landShipment.Id })">View Cargo </a>
                </td>
            </tr>
    }
</table>
